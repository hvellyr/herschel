def record Foo ( name : String, prof )

def first-name(f : Foo[]) {
  f[0].name
}

def first-proc(p : Function() -> Foo) {
  p().name
}

def gaz(x : String) -> String ...


def app.main() {
  let g : Foo
  g.name()
  g.name(force: false)

  let h : Foo
  h.name
  h.name.gaz()
  0
}



def name(f : Foo, force : Bool = true) {
  f.name
}
