def class Point(_x, _y, value: _val = 55) : Object
{
  def slot x : Int = _x
  def slot y : Int = _y
  def slot value : Int = _val

  on init(self)
  {
    value = x * y
  }

  on delete(self) nop()
}


def alias AtlasX = Atlas<Int, String>

def alias IntOfFalse = (Int, Bool == false)
