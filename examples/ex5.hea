def class Point(Object)
{
  slot x : Int = 0      , init: 0
  slot y : Int = 0      , init: 1
  slot value : Int = 55 , public, transient, key-init: x

  on init(self)
  {
    value = x * y
  }

  on delete(self) nop()
}


