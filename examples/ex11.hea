namespace heather ("eyestep/heather 1.0:core")

import(io)

def const %default-size% = 5

def some-container : HashMap(String, Int) = Vector(Int)(11)


def reduce(fn, base-value, lst)
  if (lst == #nil)
     base-value
  else
    fn(lst.car, reduce(fn, base-value, lst.cdr))


def f(lst)
  reduce(#function(o, nl) {
              if (o)
                 Pair(o, nl)
              else
                 nl
            },
         Pair(),
         lst)