def outln(s : ^String) ...

def foo = function(a : String) outln(a)

def gaz = function(a : ^String) outln(a)

def gaz2(a : ^String)
  outln(a)

def meta_gaz() -> Function(a : ^String) ...

def conv(x : ^String) -> ^String x

def app.main() {
  foo("42")
  gaz(String("hello world"))
  gaz2(String("hello world"))

  gaz2(conv(conv(String("ergo"))))

  meta_gaz()(String("hello world"))

  function(a : ^String) { outln(a) }(String("hello world"))
  0
}
