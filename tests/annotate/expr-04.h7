;; binary operations are possible with untyped expressions (for rapid prototyping)
def f0(a : Int16, b : UInt64)
  let t0 = a + b
  let t1 = a - b
  let t2 = a * b
  let t3 = a / b
  let t4 = a ** b
  let t5 = a mod b

def f5(a : UInt16, b : Int64)
  let t0 =
    (a == 0 : UInt32)
       and
    (b == 5 : Int32)
  let t1 =
    (a == (b as Integer) as Ordinal)
       or
    (b == (a as Integer) as Int64)
