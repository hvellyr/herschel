;; binary operations are possible with untyped expressions (for rapid prototyping)
def f0(a : Int, b : Float)
  let t0 = a + b
  let t1 = a - b
  let t2 = a * b
  let t3 = a / b
  let t4 = a ** b
  let t5 = a mod b

