-- binary operations are possible with untyped expressions (for rapid prototyping)
def f0(a : Complex, b : Complex)
  let t0 = a + b
  let t1 = a - b
  let t2 = a * b
  let t3 = a / b
  let t4 = a ** b
  let t5 = a mod b

def f1(a : Complex, b : Complex)
  let t0 = a == b
  let t1 = a <> b
  let t2 = a < b
  let t3 = a > b
  let t4 = a >= b
  let t5 = a <= b
  let t6 = a <=> b
  let t7 = a in b

def f4(a : Complex, b)
  let t0 = a isa Int
  let t1 = a isa b

def f5(a : Complex, b : Complex)
  -- logical and/or operators requires booleans; so for Any operands this
  -- makes explicit true comparasions necessary
  let t0 =
    (a == 3.1415 + 1i) and
    (b == 42 + 2i)
  let t1 =
    (a == b) or
    (b == a)
