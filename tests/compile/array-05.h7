import "lang/assert.h7"

def const array-size = 32

def app|main()
  let t = Int[](array-size)

  for (i = 0 then i + 1 while i < array-size)
    t[i] = i

  for (i = 0 then i + 1 while i < array-size)
    assert(t[i] == i)

  assert(t.num-items == (array-size as Integer) as Ordinal)

  let r = (t.num-items as Integer) as Int
  r
