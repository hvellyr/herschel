import "lang/assert.h7"

def const array-size = 32u

def app|main()
  let t = Ordinal[](array-size)

  for (i = 0u then i + 1 while i < array-size)
    t[i] = i

  for (i = 0u then i + 1 while i < array-size)
    assert(t[i] == i)

  assert(t.num-items == array-size)

  let r = (t.num-items as Integer) as Int
  r
